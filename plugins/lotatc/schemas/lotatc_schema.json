{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "__comments": {
            "type": "string"
        },
        "enable": {
            "type": "boolean"
        },
        "transponders": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "mode1": {
                        "type": "string"
                    },
                    "mode2": {
                        "type": "string"
                    },
                    "mode3": {
                        "type": "string"
                    },
                    "modeS": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "callsign": {
                        "type": "string"
                    },
                    "onboard": {
                        "type": "string"
                    },
                    "comment": {
                        "type": "string"
                    },
                    "force_classification": {
                        "type": "boolean"
                    },
                    "classification": {
                        "type": "object",
                        "properties": {
                            "classification": {
                                "type": "string"
                            },
                            "dimension": {
                                "type": "string"
                            },
                            "sub_dimension": {
                                "type": "string"
                            }
                        },
                        "required": ["classification"]
                    }
                },
                "additionalProperties": false
            }
        }
    },
    "required": ["enable", "transponders"],
    "additionalProperties": false
}
